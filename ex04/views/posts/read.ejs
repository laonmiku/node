<h1><%=title%></h1>

<div class="card">
    <div class="card-body">
        <div>
            <b>[<%=poster.pid%>]</b><%=poster.title%>
        </div>
        <hr>
        <div>
            
            <%=poster.contents%>
        </div>
    </div>
    <div class="card-footer" style="font-size: 12px; color: rgb(139, 139, 139);">
        <div>
            <span>작성자 : <%=poster.writer%></span>
            <span class="ms-3">작성일 : <%=poster.fdate%></span>
        </div>
    </div>
</div>